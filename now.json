{
  "name": "caravaggio",
  "version": 2,
  "public": true,
  "env": {
    "NODE_CONFIG_DIR": "/var/task/user/config"
  },
  "builds": [
    {
      "src": "index.js",
      "use": "@now/node-server",
      "config": {
        "maxLambdaSize": "50mb"
      }
    },
    {
      "src": "config/**",
      "use": "@now/static"
    },
    {
      "src": "static/**",
      "use": "@now/static"
    }
  ]
}
